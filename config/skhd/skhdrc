cmd + ctrl - y : open -na /Applications/iTerm.app
cmd + ctrl - t : open /Applications/iTerm.app
cmd + ctrl - k : open /Applications/kitty.app
cmd + ctrl - h : open -na /Applications/Google\ Chrome.app
cmd + ctrl - g : open /Applications/Google\ Chrome.app

cmd - left: yabai -m window --focus prev
cmd - right: yabai -m window --focus next 
cmd - k : yabai -m window --close
cmd - j : yabai -m window --swap first
cmd - m : yabai -m window --minimize; 
cmd - l : yabai -m window --focus first 
